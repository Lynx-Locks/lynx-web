{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,html,vue,css,less,scss,graphql,yaml,yml,json,md,mdx}\"",
    "check:lint": "next lint",
    "check:prettier": "npm run prettier -- --check",
    "check:types": "tsc --project tsconfig.json",
    "fix:prettier": "npm run prettier -- --write",
    "fix:lint": "npm run check:lint -- --fix",
    "fix": "npm-run-all --print-label fix:*",
    "validate": "npm-run-all --parallel --aggregate-output --print-label check:*"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@simplewebauthn/browser": "^9.0.1",
    "@simplewebauthn/types": "^9.0.1",
    "axios": "^1.6.7",
    "base64url": "^3.0.1",
    "jsonwebtoken": "8.5.1",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "react-select": "^5.8.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
